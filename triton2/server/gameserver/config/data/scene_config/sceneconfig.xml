		<!-- 速度检测中最大可容忍的客户端非法接入次数 -->
		<Item name = "MaxAbideCountsForClientSpeed" value = "10" type = "int" />
		<!-- 速度检测中当客户端减速时忽然的数据包数-->
		<Item name = "MaxErrorCountForClientSpeed" value = "100" type = "int" />

