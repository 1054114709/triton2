		<Level id="1" buff="0" storeboxtype="1" num="15" ></Level>
		<Level id="2" buff="70831" storeboxtype="2" num="15" ></Level>
		<Level id="3" buff="70832" storeboxtype="3" num="15" ></Level>
		<Level id="4" buff="70833" storeboxtype="4" num="15" ></Level>
	</Levels>
